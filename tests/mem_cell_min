A (((b0 && b1) || (b1 && b2) || (b2 && b0)) <-> val) W False;
A (((!b0 && !b1) || (!b1 && !b2) || (!b2 && !b0)) <-> !val) W False;

A (!w && b0 -> A X b0) W False;
A (!w && !b0 -> A X !b0) W False;
A (!w && b1 -> A X b1) W False;
A (!w && !b1 -> A X !b1) W False;
A (!w && b2 -> A X b2) W False;
A (!w && !b2 -> A X !b2) W False;

(b0 && b1 && b2) || (!b0 && !b1 && !b2);

!w;

A (EX w && EX !w) W False;

A (w && in -> A X (b0 && b1 && b2)) W False;

A (w && !in -> A X (!b0 && !b1 && !b2)) W False